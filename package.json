{
  "name": "braincrunch",
  "version": "1.2.1",
  "description": "An embeddable performant Brainfuck interpreter",
  "main": "js/index.js",
  "sideEffects": false,
  "scripts": {
    "prepare": "rimraf js && babel -s inline -d js/ src/ && flow-copy-source -v src js",
    "test": "yarn run lint && flow check && mocha",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "author": "Chris Cowan <agentme49@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Macil/braincrunch.git"
  },
  "bugs": {
    "url": "https://github.com/Macil/braincrunch/issues"
  },
  "keywords": [
    "brainfuck",
    "interpreter"
  ],
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.0",
    "eslint": "^6.0.0",
    "eslint-plugin-flowtype": "^4.0.0",
    "flow-bin": "^0.81.0",
    "flow-copy-source": "^2.0.0",
    "mocha": "^6.0.0",
    "rimraf": "^3.0.0",
    "sinon": "^8.0.2",
    "sinon-test": "^2.2.0"
  },
  "greenkeeper": {
    "ignore": [
      "flow-bin"
    ]
  }
}
